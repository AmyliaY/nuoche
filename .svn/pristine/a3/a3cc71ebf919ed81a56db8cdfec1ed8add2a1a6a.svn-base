/*当浏览器宽度大于961px*/
@media screen and (min-width:961px){
	.login_center{width:100%;height: 450px;background-color: #0F8FCB;}
}
/*当浏览器宽度小于960px的时候   设置属性 960px*/
@media screen and (max-width:960px){
	.login_center{width:960px;height: 450px;background-color: #0F8FCB;}
}
*{margin: 0px;padding: 0px;}
input{outline: none;}
input:-webkit-autofill {-webkit-box-shadow: 0 0 0px 1000px white inset;}/*去除文本框 focus颜色*/
.login_top{width:960px;height: 90px;margin: auto;}
.login_top img{width:150px;height: 60px;margin-top: 20px;float: left;}
.login_denglu{width:960px;height: 90px;margin: auto;}
.logoimg{width:450px;height: 450px;float:left;}
.login_users{width:330px;height: 380px;float: right;background-color: #F0F0F0;margin-top: 35px;}
.login_yhdl{width: 300px;height: 51px;margin: 10px auto;border-bottom:1px solid #CCC;font-size: 18px;font-weight: bold;font-family: 微软雅黑;}
.login_begin{width:290px;height: 31px;line-height: 31px;float: left;margin-top: 10px;padding-left:10px;}
.login_beginimg{width:18px;height:18px;background: url(../images/pwd-icons-new.png) -104px -49px no-repeat;display: block;float: left;margin-top: 8px;display: none;}
.login_users table{margin: auto;}
.login_users table tr{height: 45px;width:187px;list-style: 45px;font-size: 15px;font-family: 微软雅黑;}
.txtuser{width:187px;height: 35px;font-size: 15px;border:0px;font-family: 微软雅黑;}
.txtpasw{width:187px;height: 35px;font-size: 15px;border:0px;font-family: 微软雅黑;}
.txtyzm{width:120px;height: 35px;font-size: 15px;border:0px;font-family: 微软雅黑;}
.txtimg{width:56.3px;height: 35px;border:1px solid #B4B4B4;margin-left: 10px;float: left;cursor: pointer;}
.login_denglubtn{cursor:pointer;width:254px;height: 35px;border-radius:4px;border:0px;background-color: #FF6600;color:#fff;font-size: 16px;font-family: 微软雅黑;float: left;}
.login_txt{background: white;height: 35px;float: left;margin-left: 10px;border:1px solid #B4B4B4;padding-left: 5px;width:187px;}
.login_txt2{background: white;height: 35px;float: left;margin-left: 10px;border:1px solid #B4B4B4;padding-left: 5px;width:120px;}
.login_change{font-family: 微软雅黑;font-size: 16px; margin-top: 5px;}
.login_wj_jz{width: 300px;height: 61px;margin: 10px auto;border-top:1px solid #CCC;line-height: 61px;font-family: 微软雅黑;font-size: 18px;}
.login_wj{float: right;text-decoration: none;color:#000;}
input[name=login_jz]{float: left;margin-top: 25px;}
.login_jz{display: block;float: left;margin-left: 3px;}
.login_bottom{width:960px;height: 65px;margin: auto;text-align: center;}
.login_lianjie{margin: auto;width: 360px;height: 30px;}
.login_bottom ul li{list-style: none;padding: 0px 3px;float: left;padding-top: 10px;color:#666;}
.login_bottom ul li a{text-decoration: none;color:#666;font-size: 13px;font-family: 微软雅黑;}
.login_gongsixinxi{color:#666;padding-top: 10px;display: block;font-size: 13px;font-family: 微软雅黑;}
.login_shengfen{color:#0090DA;display: block;float: left;margin-top: 50px;}