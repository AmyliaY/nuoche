.ui-border-t {border-top:1px solid #dededf;}
.ui-border-b {border-bottom:1px solid #dededf;}
.ui-border-tb {border-top:#dededf 1px solid;border-bottom:#dededf 1px solid;background-image:none;}
.ui-border-l {border-left: 1px solid #dededf;}
.ui-border-r {border-right: 1px solid #dededf;}
@media screen and (-webkit-min-device-pixel-ratio: 2) {
    .ui-border-t, .ui-border-b, .ui-border-l, .ui-border-r, .ui-border-tb {border:0;}
    .ui-border-t {background-position:left top;background-image:-webkit-gradient(linear,left bottom,left top,color-stop(0.5,transparent),color-stop(0.5,#dededf),to(#dededf));}
    .ui-border-b {background-position: left bottom;background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #dededf), to(#dededf)); }
    .ui-border-t, .ui-border-b, .ui-border-tb {background-repeat:repeat-x;-webkit-background-size:100% 1px;}
    .ui-border-tb {background-image:-webkit-gradient(linear,left bottom,left top,color-stop(0.5,transparent),color-stop(0.5,#dededf), to(#dededf)), -webkit-gradient(linear,left top,left bottom,color-stop(0.5,transparent),color-stop(0.5,#dededf),to(#dededf));background-position:top, bottom; }
    .ui-border-l {background-position:left top;background-image:-webkit-gradient(linear, right top, left top, color-stop(0.5,transparent), color-stop(0.5, #dededf), to(#dededf)); }
    .ui-border-r {background-position:right top;background-image: -webkit-gradient(linear, left top, right top, color-stop(0.5,transparent), color-stop(0.5, #dededf), to(#dededf)); }
    .ui-border-l, .ui-border-r {background-repeat:repeat-y;-webkit-background-size:1px 100%;}
}

