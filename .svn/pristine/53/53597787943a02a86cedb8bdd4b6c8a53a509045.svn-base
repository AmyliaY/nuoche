body {
	font-family: "微软雅黑";
	font-family: 'Helvetica Neue', Helvetica, sans-serif;
	font-size: 16px;
	color: #000;
	background-color: #efeff4;
	-webkit-overflow-scrolling: touch;
}

.mui-bar {
	padding-left: initial;
	padding-right: initial;
	box-shadow: none;
}

.mui-bar-nav {
	box-shadow: none;
}

.image_back {
	width: 28px;
	height: 28;
	margin-top: 8px;
	margin-right: 15px;
	margin-left: 5px;
}

header a {
	text-decoration: none;
}

header h1 span img {
	width: 20px;
	height: 25px;
	margin-left: -5px;
}


/*去掉header中的下边的框的虚影*/

.mui-content {
	background-color: #F0F0F0;
}

.warp {
	padding-top: 1.5rem;
	padding-bottom: 1.9rem;
}

.warptwo {
	padding-bottom: 3.7rem;
}

.warpthree {
	padding-bottom: 0;
}

.clearfloat:after {
	display: block;
	clear: both;
	content: "";
	visibility: hidden;
	height: 0
}

.clearfloat {
	zoom: 1
}

.h-topone {
	width: 100%;
	text-align: center;
	padding: 3% 5% 5%;
}

.h-top {
	width: 100%;
	background-color: #6CCCCA;
	padding: 10% 5%;
}

.h-topone p {
	font-size: 1.6rem;
	color: #fff;
}

p {
	margin-top: 0;
	color: #8f8f94;
}

.h-topone p samp {
	font-size: 0.45rem;
	font-family: 微软雅黑;
}

.h-topone span {
	font-size: 1.00rem;
	/*  .45   */
	color: #fff;
}

.clearfloat:after {
	display: block;
	clear: both;
	content: "";
	visibility: hidden;
	height: 0;
}

.recharge {
	width: 100%;
}

.recharge .czhi {
	width: 100%;
	padding: 0 3%;
	background-color: #fff;
	height: 45px;
	line-height: 45px;
	margin-top: 3%;
}

.recharge .czhi p,
.recharge .czhi span {
	width: 20%;
	float: left;
	font-size: 0.9rem;
	color: #333333;
}

p {
	font-size: 14px;
	margin-top: 0;
	margin-bottom: 10px;
	color: #8f8f94;
}

.recharge .czhi input {
	margin: 0;
	padding: 0;
	height: 2.8rem;
	float: left;
	width: 70%;
	border: none;
	font-size: 1rem;
	color: #333333;
}

input[type=color],
input[type=date],
input[type=datetime-local],
input[type=datetime],
input[type=email],
input[type=month],
input[type=number],
input[type=password],
input[type=search],
input[type=tel],
input[type=text],
input[type=time],
input[type=url],
input[type=week],
select,
textarea {
	line-height: 21px;
	width: 100%;
	height: 40px;
	margin-bottom: 15px;
	padding: 10px 15px;
	-webkit-user-select: text;
	border: 1px solid rgba(0, 0, 0, .2);
	border-radius: 3px;
	outline: 0;
	background-color: #fff;
	-webkit-appearance: none;
}

.recharge .czhi span {
	display: block;
	width: 10%;
	text-align: right;
}

.recharge .guize {
	width: 96%;
	margin: 3% auto;
	background-color: #fff;
	border: 1px solid #e1e1e1;
	padding: 0 3%;
}

.ra3 {
	-webkit-border-radius: 3px;
	border-radius: 3px;
}

.recharge .guize h3 {
	width: 100%;
	border-bottom: 1px solid #e1e1e1;
	height: 1.5rem;
	line-height: 1.5rem;
	font-size: .5rem;
	color: #333333;
	text-align: center;
	font-weight: normal;
}

.mui-h3,
h3 {
	font-size: 24px;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	line-height: 1;
	margin-top: 5px;
	margin-bottom: 5px;
}

.recharge .guize .content {
	padding: 5% 0;
	color: #666666;
	font-size: 0.8rem;
	line-height: 1.3rem;
	text-indent: 2em;
}

.recharge .bottom {
	width: 96%;
	margin: 0 auto 5%;
}

.recharge .bottom .btn {
	width: 48%;
	text-align: center;
	height: 1.5rem;
	line-height: 1.5rem;
	background-color: #ff8b23;
	font-size: .5rem;
	color: #fff;
}

.ra3 {
	-webkit-border-radius: 3px;
	border-radius: 3px;
}

.db {
	display: block;
}

.fl {
	float: left;
	display: inline;
}

a {
	text-decoration: none;
	color: #007aff;
}

a {
	background: 0 0;
}

a,
button,
textarea,
select {
	-webkit-appearance: none;
	-webkit-tap-highlight-color: rgba(255, 0, 0, 0);
}

a {
	text-decoration: none;
	outline: none;
}

.recharge .bottom .btn:last-child {
	background-color: #d4d4d4;
	color: #666666;
}

.recharge .bottom .btn {
	width: 48%;
	text-align: center;
	height: 2.5rem;
	line-height: 2.5rem;
	background-color: #ff8b23;
	font-size: 1.3rem;
	color: #fff;
}

.ra3 {
	-webkit-border-radius: 3px;
	border-radius: 3px;
}

.fr {
	float: right;
	display: inline;
}