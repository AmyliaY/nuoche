	<!-- *****唐仁鑫  注入   定时生成二维码参数 *********************************************************** -->
	<bean id="qrService" class="com.service.admin.QRCodeService">
	   <property name="hqlDAO" ref="HqlDAO"></property>
	   <property name="qrCodePath" value="E:\WorkSpace2016\.metadata\.me_tcat7\webapps\nuoche\qrcodeimage"></property>
